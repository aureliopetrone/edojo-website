{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch Prisma Studio",
      "type": "node",
      "request": "launch",
      "program": "${workspaceFolder}/node_modules/prisma/build/index.js",
      "args": ["studio"],
      "cwd": "${workspaceFolder}",
      "env": {
        "NODE_ENV": "development"
      },
      "console": "integratedTerminal"
    }
  ]
}